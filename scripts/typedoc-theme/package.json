{
  "name": "@finos/legend-internal-typedoc-theme",
  "private": true,
  "description": "Custom TypeDoc theme for finos/legend-studio repository",
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "scripts/typedoc-theme"
  },
  "license": "Apache-2.0",
  "scripts": {
    "clean": "yarn clean:cache",
    "clean:cache": "rimraf \"build\"",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=requireConfigFile:false \"./*.{mjs,cjs,js}\" \"src/**/*.{mjs,cjs,js}\""
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "rimraf": "4.4.0",
    "typedoc": "0.23.27"
  }
}
