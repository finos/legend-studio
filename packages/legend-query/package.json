{
  "name": "@finos/legend-query",
  "version": "1.3.3",
  "description": "Legend Query core",
  "keywords": [
    "legend",
    "legend-query",
    "query",
    "query builder"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-query",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-query"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:sass && yarn build:ts",
    "build:sass": "cross-env INIT_CWD=$INIT_CWD node ../../scripts/workflow/buildSass.js",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "npm-run-all clean:cache clean:lib",
    "clean:cache": "rimraf \"build\"",
    "clean:lib": "rimraf \"lib\"",
    "dev": "npm-run-all --parallel dev:sass dev:ts",
    "dev:sass": "sass style:lib --watch --load-path=../../node_modules/@finos/legend-art/scss",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "27.1.0",
    "@ag-grid-community/core": "27.1.0",
    "@ag-grid-community/react": "27.1.0",
    "@finos/legend-application": "workspace:*",
    "@finos/legend-art": "workspace:*",
    "@finos/legend-graph": "workspace:*",
    "@finos/legend-model-storage": "workspace:*",
    "@finos/legend-server-depot": "workspace:*",
    "@finos/legend-shared": "workspace:*",
    "@testing-library/react": "12.1.4",
    "@types/papaparse": "5.3.2",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-router-dom": "5.3.2",
    "date-fns": "2.28.0",
    "history": "5.3.0",
    "mobx": "6.5.0",
    "mobx-react-lite": "3.3.0",
    "monaco-editor": "0.33.0",
    "papaparse": "5.3.2",
    "react": "17.0.2",
    "react-dnd": "15.1.1",
    "react-dnd-html5-backend": "15.1.2",
    "react-dom": "17.0.2",
    "react-hotkeys": "2.0.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "@testing-library/dom": "8.12.0",
    "cross-env": "7.0.3",
    "eslint": "8.12.0",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "sass": "1.49.9",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  },
  "extensions": {
    "graphPreset": "@finos/legend-graph-preset-query-builder",
    "pureProtocolProcessorPlugin": "@finos/legend-graph-plugin-query-builder-pure-protocol-processor"
  }
}
