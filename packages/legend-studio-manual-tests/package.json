    "test:manual": "jest --config jest.config.manual.js",
    "test:watch": "jest --config jest.config.manual.js --watch"
  },
  "dependencies": {
    "@finos/legend-studio": "workspace:*",
    "@finos/legend-studio-shared": "workspace:*",
    "@types/react": "17.0.6",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@finos/legend-studio-dev-utils": "workspace:*",
    "axios": "0.21.1",
    "cross-env": "7.0.3",
    "eslint": "7.27.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.2.4"
  }
}
