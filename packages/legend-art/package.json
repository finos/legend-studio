{
  "name": "@finos/legend-art",
  "version": "7.0.13",
  "description": "Legend shared visual components and component utilities",
  "keywords": [
    "legend",
    "component",
    "shared-component",
    "utilities"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-art",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-art"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./markdown/test/MockedReactMarkdown.js": "./lib/markdown/__test-utils__/MockedReactMarkdown.js",
    "./markdown/test/MockedRemarkGFM.js": "./lib/markdown/__test-utils__/MockedRemarkGFM.js",
    "./lib/index.css": "./lib/index.css"
  },
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:sass && yarn build:ts",
    "build:sass": "cross-env INIT_CWD=$INIT_CWD node ../../scripts/workflow/buildSass.js",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "npm-run-all clean:cache clean:lib",
    "clean:cache": "rimraf \"build\"",
    "clean:lib": "rimraf \"lib\"",
    "dev": "npm-run-all --parallel dev:sass dev:ts",
    "dev:sass": "sass style:lib --watch --load-path=../../node_modules/@finos/legend-art/scss",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@finos/legend-shared": "workspace:*",
    "@fontsource/raleway": "4.5.12",
    "@fontsource/roboto": "4.5.8",
    "@fontsource/roboto-condensed": "4.5.9",
    "@fontsource/roboto-mono": "4.5.10",
    "@mui/material": "5.12.3",
    "@types/react": "18.2.6",
    "@types/react-select": "4.0.18",
    "@types/react-window": "1.8.5",
    "clsx": "1.2.1",
    "mobx": "6.9.0",
    "mobx-react-lite": "3.4.3",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.5",
    "react-icons": "4.8.0",
    "react-markdown": "8.0.7",
    "react-reflex": "4.1.0",
    "react-resize-detector": "9.1.0",
    "react-select": "4.3.1",
    "react-window": "1.8.9",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "@jest/globals": "29.5.0",
    "cross-env": "7.0.3",
    "eslint": "8.40.0",
    "jest": "29.5.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.0",
    "sass": "1.62.1",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  }
}
