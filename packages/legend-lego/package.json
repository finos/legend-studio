{
  "name": "@finos/legend-lego",
  "version": "2.0.19",
  "description": "Legend code editor support",
  "keywords": [
    "legend",
    "code-editor"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-lego",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-lego"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./application": "./lib/application/index.js",
    "./code-editor": "./lib/code-editor/index.js",
    "./code-editor/test": "./lib/code-editor/__test__.js",
    "./code-editor/test/MockedMonacoEditor.js": "./lib/code-editor/__test-utils__/MockedMonacoEditor.js",
    "./data-grid": "./lib/data-grid/index.js",
    "./graph-editor": "./lib/graph-editor/index.js",
    "./lib/index.css": "./lib/index.css"
  },
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:sass && yarn build:ts",
    "build:sass": "cross-env INIT_CWD=$INIT_CWD node ../../scripts/workflow/buildSass.js",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "npm-run-all clean:cache clean:lib",
    "clean:cache": "rimraf \"build\"",
    "clean:lib": "rimraf \"lib\"",
    "dev": "npm-run-all --parallel dev:sass dev:ts",
    "dev:sass": "sass style:lib --watch --load-path=../../node_modules/@finos/legend-art/scss",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "32.3.3",
    "@ag-grid-community/core": "32.3.3",
    "@ag-grid-community/csv-export": "32.3.3",
    "@ag-grid-community/react": "32.3.3",
    "@ag-grid-community/styles": "32.3.3",
    "@ag-grid-enterprise/clipboard": "32.3.3",
    "@ag-grid-enterprise/column-tool-panel": "32.3.3",
    "@ag-grid-enterprise/core": "32.3.3",
    "@ag-grid-enterprise/excel-export": "32.3.3",
    "@ag-grid-enterprise/filter-tool-panel": "32.3.3",
    "@ag-grid-enterprise/menu": "32.3.3",
    "@ag-grid-enterprise/range-selection": "32.3.3",
    "@ag-grid-enterprise/row-grouping": "32.3.3",
    "@ag-grid-enterprise/server-side-row-model": "32.3.3",
    "@ag-grid-enterprise/side-bar": "32.3.3",
    "@ag-grid-enterprise/status-bar": "32.3.3",
    "@finos/legend-application": "workspace:*",
    "@finos/legend-art": "workspace:*",
    "@finos/legend-code-editor": "workspace:*",
    "@finos/legend-graph": "workspace:*",
    "@finos/legend-shared": "workspace:*",
    "@types/css-font-loading-module": "0.0.14",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "mobx": "6.13.5",
    "mobx-react-lite": "4.0.7",
    "monaco-editor": "0.52.0",
    "react": "18.3.1",
    "react-dnd": "16.0.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "cross-env": "7.0.3",
    "eslint": "9.15.0",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "sass": "1.81.0",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  }
}
