{
  "name": "@finos/stylelint-config-legend-studio",
  "version": "0.0.0",
  "description": "Legend Studio Stylelint config",
  "keywords": [
    "legend",
    "legend-studio",
    "studio",
    "stylelint",
    "stylelint-config"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/stylelint-config",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/stylelint-config"
  },
  "license": "Apache-2.0",
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf \"build\"",
    "fix:js": "yarn lint:js --fix",
    "lint:js": "cross-env NODE_ENV=ci eslint --cache --cache-location ./build/.eslintcache --parser-options=requireConfigFile:false \"./*.js\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js"
  },
  "dependencies": {
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "21.0.0",
    "stylelint-scss": "3.19.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint": "7.21.0",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "stylelint": "^13.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  }
}
