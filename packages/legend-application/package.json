{
  "name": "@finos/legend-application",
  "version": "16.0.23",
  "description": "Legend application core",
  "keywords": [
    "legend",
    "legend-application",
    "application",
    "legend-application-core"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-application",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-application"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./browser": "./lib/browser.js",
    "./test": "./lib/__test__.js",
    "./lib/index.css": "./lib/index.css"
  },
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:sass && yarn build:ts",
    "build:sass": "cross-env INIT_CWD=$INIT_CWD node ../../scripts/workflow/buildSass.js",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "npm-run-all clean:cache clean:lib",
    "clean:cache": "rimraf \"build\"",
    "clean:lib": "rimraf \"lib\"",
    "dev": "npm-run-all --parallel dev:sass dev:ts",
    "dev:sass": "sass style:lib --watch --load-path=../../node_modules/@finos/legend-art/scss",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@finos/legend-art": "workspace:*",
    "@finos/legend-shared": "workspace:*",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/react-router-dom": "5.3.3",
    "mobx": "6.13.6",
    "mobx-react-lite": "4.1.0",
    "react": "19.0.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "19.0.0",
    "react-router": "7.1.5",
    "react-router-dom": "7.1.5",
    "serializr": "3.0.3",
    "xterm": "5.3.0",
    "xterm-addon-fit": "0.8.0",
    "xterm-addon-search": "0.13.0",
    "xterm-addon-unicode11": "0.6.0",
    "xterm-addon-webgl": "0.16.0"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "cross-env": "7.0.3",
    "eslint": "9.19.0",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "sass": "1.83.4",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  },
  "extensions": {
    "applicationPlugin": "@finos/legend-application-plugin-core",
    "iframeEventNotifierPlugin": "@finos/legend-event-notifier-plugin-iframe"
  }
}
