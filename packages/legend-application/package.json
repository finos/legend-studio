{
  "name": "@finos/legend-application",
  "version": "1.0.2",
  "description": "Legend application core",
  "keywords": [
    "legend",
    "legend-application",
    "application",
    "legend-application-core"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-application",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-application"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:sass && yarn build:tsc",
    "build:sass": "cross-env INIT_CWD=$INIT_CWD node ../../scripts/workflow/buildSass.js",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf \"lib\" \"build\"",
    "dev": "npm-run-all --parallel dev:sass dev:tsc",
    "dev:sass": "sass style lib --watch --load-path=../../node_modules/@finos/legend-art/scss",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@finos/legend-art": "workspace:*",
    "@finos/legend-graph": "workspace:*",
    "@finos/legend-shared": "workspace:*",
    "@material-ui/core": "4.12.3",
    "@testing-library/react": "12.1.2",
    "@types/css-font-loading-module": "0.0.7",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "history": "5.1.0",
    "mobx": "6.3.8",
    "mobx-react-lite": "3.2.2",
    "monaco-editor": "0.30.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-resize-detector": "6.7.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "serializr": "2.0.5"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "@testing-library/dom": "8.11.1",
    "cross-env": "7.0.3",
    "eslint": "8.4.0",
    "jest": "27.4.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "sass": "1.44.0",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  }
}
