{
  "name": "@finos/legend-shared",
  "version": "2.0.1",
  "description": "Legend Studio shared utilities and helpers",
  "keywords": [
    "legend",
    "legend-studio",
    "studio",
    "utilities",
    "utils"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-shared",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-shared"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": "./lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "npm-run-all clean:cache clean:lib",
    "clean:cache": "rimraf \"build\"",
    "clean:lib": "rimraf \"lib\"",
    "dev": "yarn dev:ts",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/lodash-es": "4.17.6",
    "@types/lossless-json": "1.0.1",
    "@types/object-hash": "2.2.1",
    "@types/pako": "2.0.0",
    "@types/seedrandom": "3.0.2",
    "@types/uuid": "8.3.4",
    "hash.js": "1.1.7",
    "http-status-codes": "2.2.0",
    "lodash-es": "4.17.21",
    "lossless-json": "1.0.5",
    "mobx": "6.5.0",
    "object-hash": "3.0.0",
    "pako": "2.0.4",
    "pretty-format": "28.1.0",
    "query-string": "7.1.1",
    "seedrandom": "3.0.5",
    "serializr": "2.0.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "cross-env": "7.0.3",
    "eslint": "8.15.0",
    "jest": "28.1.0",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.6.4"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  },
  "extensions": {
    "iframeEventNotifierPlugin": "@finos/legend-event-notifier-plugin-iframe",
    "webConsoleLoggerPlugin": "@finos/legend-logger-plugin-web-console"
  }
}
