{
  "name": "@finos/legend-studio-shared",
  "version": "0.0.7",
  "description": "Legend Studio shared utilities and helpers",
  "keywords": [
    "legend",
    "legend-studio",
    "studio",
    "utilities",
    "utils"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-studio-shared",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-studio-shared"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:tsc",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf \"lib\" \"build\"",
    "dev": "dev:tsc",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/lodash-es": "4.17.4",
    "@types/lossless-json": "1.0.0",
    "@types/object-hash": "2.1.0",
    "@types/pako": "1.0.1",
    "@types/seedrandom": "3.0.0",
    "@types/uuid": "8.3.0",
    "hash.js": "1.1.7",
    "http-status-codes": "2.1.4",
    "lodash-es": "4.17.21",
    "lossless-json": "1.0.4",
    "mobx": "6.2.0",
    "object-hash": "2.1.1",
    "pako": "2.0.3",
    "query-string": "7.0.0",
    "seedrandom": "3.0.5",
    "serializr": "2.0.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@finos/legend-studio-dev-utils": "workspace:*",
    "cross-env": "7.0.3",
    "eslint": "7.24.0",
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "rimraf": "3.0.2",
    "typescript": "4.2.4"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  }
}
