{
  "name": "legend-studio",
  "description": "Legend Studio",
  "repository": "https://github.com/finos/legend-studio",
  "engines": {
    "node": ">=14.3.0"
  },
  "scripts": {
    "setup": "node ./dev/scripts/setup.js",
    "build": "webpack --mode production",
    "build:analyze": "webpack --mode production --env.analyze",
    "start": "node ./dev/scripts/start.js",
    "start:adv-lint": "node ./dev/scripts/start.js --advanced-linting",
    "start:minimal": "webpack-dev-server --mode development --hot",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --config ./.eslintrc-advanced.js --rulesdir ./dev/eslint_rules dev style app",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --config ./.eslintrc-advanced.js --rulesdir ./dev/eslint_rules --fix dev style app",
    "clean:test": "jest --clearCache",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage --verbose",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --watch --verbose",
    "test:seq:watch": "jest --runInBand --watch --verbose",
    "test:full": "npm run clean:test && npm run test:coverage",
    "upgrade": "npm-check --update ---save-exact",
    "test:e2e": "cypress open"
  },
